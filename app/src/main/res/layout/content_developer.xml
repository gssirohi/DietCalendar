<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.techticz.app.ui.activity.DeveloperActivity"
    tools:showIn="@layout/activity_developer"
    android:layout_height="match_parent">
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingTop="48dp"
    android:orientation="vertical">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        style="?attr/textAppearanceHeadline1"
        android:text="H1"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        style="?attr/textAppearanceHeadline2"
        android:text="H2"/>
    <com.google.android.material.tabs.TabLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <com.google.android.material.tabs.TabItem
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TBA1"/>
        <com.google.android.material.tabs.TabItem
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TBA2"/>
        <com.google.android.material.tabs.TabItem
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TBA3"/>
    </com.google.android.material.tabs.TabLayout>
    <androidx.appcompat.widget.AppCompatCheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CheckBox androidx"/>
    <androidx.appcompat.widget.AppCompatCheckedTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Checked TextView"
        android:checked="true"/>
    <com.google.android.material.card.MaterialCardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="8dp"
        app:contentPadding="8dp"
        android:theme="@style/Surface"
        >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            style="?attr/textAppearanceHeadline3"
            android:text="Headli3"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            style="?attr/textAppearanceHeadline4"
            android:text="Headli4"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textAppearance="?attr/textAppearanceListItem"
            android:text="textAppearanceListItem"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textAppearance="@style/TextAppearance.MaterialComponents.Caption"
            android:text="textAppearanceListItem"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textAppearance="@style/TextAppearance.MaterialComponents.Headline6"
            android:text="textAppearanceListItem"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            style="?attr/textAppearanceListItemSecondary"
            android:text="textAppearanceListItemSeconday"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            style="?attr/textAppearanceListItemSmall"
            android:text="textAppearanceListItemSmall"/>


        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            style="?attr/textAppearanceSmallPopupMenu"
            android:text="textAppearanceSmallPopupMenu"/>
        <com.google.android.material.button.MaterialButton
            android:layout_weight="1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            style="@style/Widget.MaterialComponents.Button.OutlinedButton.Icon"
            app:icon="@drawable/ic_clear"
            android:text="Dummy Button"/>
        <com.google.android.material.chip.Chip
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Chip"/>
        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/Widget.AppCompat.CompoundButton.RadioButton"
            android:text="Radio Button"/>
        <com.google.android.material.textfield.TextInputLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="Text Input Hint">
            <com.google.android.material.textfield.TextInputEditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="This is Edit Text"/>
        </com.google.android.material.textfield.TextInputLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <com.github.ybq.android.spinkit.SpinKitView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                style="@style/SpinKitView.Small.Wave"
                app:SpinKit_Color="@color/secondaryColor"
                />
            <com.github.ybq.android.spinkit.SpinKitView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                style="@style/SpinKitView.Small.ChasingDots"
                app:SpinKit_Color="@color/secondaryColor"
                />
            <com.github.ybq.android.spinkit.SpinKitView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                style="@style/SpinKitView.Small.Circle"
                app:SpinKit_Color="@color/secondaryColor"
                />
            <com.github.ybq.android.spinkit.SpinKitView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                style="@style/SpinKitView.Small.CubeGrid"
                app:SpinKit_Color="@color/secondaryColor"
                />
            <com.github.ybq.android.spinkit.SpinKitView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                style="@style/SpinKitView.Small.Pulse"
                app:SpinKit_Color="@color/secondaryColor"
                />
            <com.github.ybq.android.spinkit.SpinKitView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                style="@style/SpinKitView.Small.DoubleBounce"
                app:SpinKit_Color="@color/secondaryColor"
                />
            <com.github.ybq.android.spinkit.SpinKitView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                style="@style/SpinKitView.Small.WanderingCubes"
                app:SpinKit_Color="@color/secondaryColor"
                />

        </LinearLayout>


    </LinearLayout>

    </com.google.android.material.card.MaterialCardView>
    <com.google.android.material.card.MaterialCardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="8dp"
        app:contentPadding="8dp"
        >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                style="?attr/textAppearanceOverline"
                android:text="textAppearanceOverline"/>
            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    style="?attr/textAppearanceHeadline5"
                    android:text="Headli5"/>
                <TextView
                    android:layout_weight="1"
                    android:gravity="right"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    style="?attr/textAppearanceHeadline6"
                    android:text="Headli6"/>
            </LinearLayout>


            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                style="?attr/textAppearanceSubtitle1"
                android:text="Subtitle1"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                style="?attr/textAppearanceBody1"
                android:text="Body1 sjdhsjf dfjh djfh jdf d jfh jsidjikj sdkj sdj  skhd js hdjs hdjs hdj sd jh sd jsd hjs dh"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:layout_gravity="right"
                android:gravity="right"
                style="?attr/textAppearanceButton"
                android:text="BUTTON"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                style="?attr/textAppearanceSubtitle2"
                android:text="Subtitle2"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                style="?attr/textAppearanceBody2"
                android:text="Body2 skjfd kfdk fdkjjdkf jkdjf kd jfkdhf jd hfkdh fidh fkhdkf hidhf kdhf kdhf khf kfh fkdh "/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:layout_gravity="right"
                android:gravity="right"
                style="?attr/textAppearanceButton"
                android:text="BUTTON"/>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
            <TextView
                android:layout_weight="4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                style="?attr/textAppearanceCaption"
                android:layout_marginTop="8dp"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="This is Caption text"/>


            <com.google.android.material.button.MaterialButton
                android:layout_weight="1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="right"
                android:text="Dummy Button"/>
            </LinearLayout>
        </LinearLayout>
    </com.google.android.material.card.MaterialCardView>
    <Button
        android:id="@+id/bt_all"
        android:layout_width="300dp"
        android:layout_height="48dp"
        android:layout_gravity="center"
        android:layout_margin="8dp"
        android:text="Upload All"/>

    <Button
        android:id="@+id/bt_foods"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_margin="8dp"
        android:text="Upload Foods"/>

    <Button
        android:id="@+id/bt_recipes"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_margin="8dp"
        android:text="Upload Recipes"/>

    <Button
        android:id="@+id/bt_plates"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_margin="8dp"
        android:text="Upload Plates"/>

    <Button
        android:id="@+id/bt_plans"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_margin="8dp"
        android:text="Upload Plans"/>
    <Button
        android:id="@+id/bt_food_categories"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_margin="8dp"
        android:text="Upload Food categories"/>

    <Button
        android:id="@+id/bt_recipe_categories"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_margin="8dp"
        android:text="Upload Recipe categories"/>

    <Button
        android:id="@+id/bt_serving_types"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_margin="8dp"
        android:text="Upload Serving Types"/>

    <Button
        android:id="@+id/bt_units"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_margin="8dp"
        android:text="Upload Units"/>

    <Button
        android:id="@+id/bt_nutrient_units"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_margin="8dp"
        android:text="Upload Nutrient Units"/>

</LinearLayout>
</androidx.core.widget.NestedScrollView>